{

  "ext": "js, json, hbs",

  "watch": [
    "/helpers/*",
    "/modules/*",
    "/routes/*",
    "/templates/*"
  ],

  "ignore": [
    "/_hbs/*",
    "/_helpers/*",
    "/_js/*",
    "/_scss/*",
    "/node_modules/*",
    "/public/*",
    ".babelrc",
    ".editorconfig",
    ".eslintrc",
    ".gitignore",
    "_config.js",
    "gulpfile.js",
    "package.json",
    "webpack.config.js"
  ]

}
